DoubleQuotes:
    regex: ['/"/', '/(?<=[^\\])"/']
    rules:
        - must_not be_in SingleQuotes
        
SingleQuotes:
    regex: ["/'/", "/(?<=[^\\])'/"]
    rules:
        - must_not be_in DoubleQuotes
        
Whitespace:
    regex: /\s+/
    rules:
        - must_not be_in DoubleQuotes
        - must_not be_in SingleQuotes
        
Block:
    regex: [/{/, /}/]
    rules:
        - must_not be_in DoubleQuotes
        - must_not be_in SingleQuotes
        - must_not be_in Group
        
Group:
    regex: [/(/, /)/]
    rules:
        - must_not be_in DoubleQuotes
        - must_not be_in SingleQuotes
