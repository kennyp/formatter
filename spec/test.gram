---
name: DoubleQuotes
regex: [/"/, /(?<=[^\\])"/]
rules:
    - must_not be_in SingleQuotes
---
name: SingleQuotes
regex: [/'/, /(?<=[^\\])'/]
rules:
    - must_not be_in DoubleQuotes
---
name: Whitespace
regex: /\s+/
rules:
    - must_not be_in DoubleQuotes
    - must_not be_in SingleQuotes
---
name: Block
regex: [/{/, /}/]
rules:
    - must_not be_in DoubleQuotes
    - must_not be_in SingleQuotes
    - must_not be_in Group
---
name: Group
regex: [/(/, /)/]
rules:
    - must_not be_in DoubleQuotes
    - must_not be_in SingleQuotes